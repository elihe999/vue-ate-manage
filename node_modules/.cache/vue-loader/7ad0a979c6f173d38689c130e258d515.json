e', this.handleBus);\r\n        },\r\n        methods: {\r\n            handleListener(){\r\n                bus.$on('collapse', this.handleBus);\r\n                // 调用renderChart方法对图表进行重新渲染\r\n                window.addEventListener('resize', this.renderChart)\r\n            },\r\n            handleBus(msg){\r\n                setTimeout(() => {\r\n                    this.renderChart()\r\n                }, 300);\r\n            },\r\n            renderChart(){\r\n                this.$refs.bar.renderChart();\r\n                this.$refs.line.renderChart();\r\n            },\r\n            getData() {\r\n                this.url = \"/index.php/Welcome/return_test\"\r\n                this.$axios.post(this.url).then((res) => {\r\n                    let temp = []\r\n                    let temp_suite = []\r\n                    res.data.list.forEach(function(currentValue) {\r\n                        let timeObj = new Date(parseInt(currentValue['time']) * 1000)\r\n                        let year = timeObj.getFullYear()\r\n                        let mouth = timeObj.getMonth() + 1\r\n                        let day = timeObj.getDate()\r\n                        let temp_time = year + '/' + mouth + '/' + day\r\n                        temp.push({'value':currentValue['failnum'],'name':temp_time})\r\n                        temp_suite.push({'value':currentValue['totalnum'],'name':temp_time})\r\n                    })\r\n                    this.data = temp\r\n                    this.data1 = temp_suite\r\n                })\r\n            }\r\n        }\r\n    }\r\n\r\n</script>\r\n\r\n\r\n<style scoped>\r\n    .el-row {\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .grid-content {\r\n        display: flex;\r\n        align-items: center;\r\n        height: 100px;\r\n    }\r\n\r\n    .grid-cont-right {\r\n        flex: 1;\r\n        text-align: center;\r\n        font-size: 14px;\r\n        color: #999;\r\n    }\r\n\r\n    .grid-num {\r\n        font-size: 30p